{
  "name": "@vue-mapp-kit/esri",
  "version": "5.0.0-rc.0",
  "description": "A component driven approach to managing Esri ArcGIS 4.x objects",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/matt-e-king/vue-mapp-kit"
  },
  "author": {
    "name": "CALS CCT",
    "email": "webdev@cals.arizona.edu"
  },
